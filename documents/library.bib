@article{sorba_using_2010,
	title = {{USING} {THE} 1.6 Î¼m {BUMP} {TO} {STUDY} {REST}-{FRAME} {NEAR}-{INFRARED}-{SELECTED} {GALAXIES} {AT} {REDSHIFT} 2},
	volume = {721},
	issn = {0004-637X, 1538-4357},
	url = {https://iopscience.iop.org/article/10.1088/0004-637X/721/2/1056},
	doi = {10.1088/0004-637X/721/2/1056},
	abstract = {We explore the feasibility and limitations of using the 1.6 Î¼m bump as a photometric redshift indicator and selection technique, and use it to study the rest-frame H-band galaxy luminosity and stellar mass functions (SMFs) at redshift z â¼ 2. We use publicly available Spitzer/IRAC images in the GOODS ï¬elds and ï¬nd that color selection in the IRAC bandpasses alone is comparable in completeness and contamination to BzK selection. We ï¬nd that the shape of the 1.6 Î¼m bump is robust, and photometric redshifts are not greatly affected by choice of model parameters. Comparison with spectroscopic redshifts shows photometric redshifts to be reliable. We create a rest-frame NIRselected catalog of galaxies at z â¼ 2 and construct a galaxy SMF. Comparisons with other SMFs at approximately the same redshift but determined using shorter wavelengths show good agreement. This agreement suggests that selection at bluer wavelengths does not miss a signiï¬cant amount of stellar mass in passive galaxies. Comparison with SMFs at other redshifts shows evidence for the downsizing scenario of galaxy evolution. We conclude by pointing out the potential for using the 1.6 Î¼m bump technique to select high-redshift galaxies with the JWST, whose Î» {\textgreater} 0.6 Î¼m coverage will not be well suited to selecting galaxies using techniques that require imaging at shorter wavelengths.},
	language = {en},
	number = {2},
	urldate = {2021-10-04},
	journal = {\apj},
	author = {Sorba, Robert and Sawicki, Marcin},
	month = oct,
	year = {2010},
	pages = {1056--1078},
}

