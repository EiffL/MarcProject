@article{rettura_candidate_2014,
	title = {{CANDIDATE} {CLUSTERS} {OF} {GALAXIES} {AT} \textit{z} {\textgreater} 1.3 {IDENTIFIED} {IN} {THE} {SPITZER} {SOUTH} {POLE} {TELESCOPE} {DEEP} {FIELD} {SURVEY}},
