	issn = {0004-637X, 1538-4357},
	url = {http://arxiv.org/abs/1403.2390},
	doi = {10.1088/0004-637X/786/1/17},
	abstract = {We present 4.5 Âµm luminosity functions for galaxies identiï¬ed in 178 candidate galaxy clusters at 1.3 {\textless} z {\textless} 3.2. The clusters were identiï¬ed as Spitzer/IRAC color-selected overdensities in the Clusters Around Radio-Loud AGN (CARLA) project, which imaged 421 powerful radio-loud AGN at z {\textgreater} 1.3. The luminosity functions are derived for diï¬erent redshift and richness bins, and the IRAC imaging reaches depths of mâ + 2, allowing us to measure the faint end slopes of the luminosity functions. We ï¬nd that Î± = â1 describes the luminosity function very well in all redshifts bins and does not evolve signiï¬cantly. This provides evidence that the rate at which the low mass galaxy population grows through star formation, gets quenched and is replenished by in-falling ï¬eld galaxies does not have a major net eï¬ect on the shape of the luminosity function. Our measurements for mâ are consistent with passive evolution models and high formation redshifts (zf â¼ 3). We ï¬nd a slight trend towards fainter mâ for the richest clusters, implying that the most massive clusters in our sample could contain older stellar populations, yet another example of cosmic downsizing. Modelling shows that a contribution of a star-forming population of up to 40 \% cannot be ruled out. This value, found from our targeted survey, is signiï¬cantly lower than the values found for slightly lower redshift, z â¼ 1, clusters found in wide-ï¬eld surveys. The results are consistent with cosmic downsizing, as the clusters studied here were all found in the vicinity of radio-loud AGNs â which have proven to be preferentially located in massive dark matter halos in the richest environments at high redshift â and may therefore be older and more evolved systems than the general protocluster population.},
	language = {en},
	number = {1},
	urldate = {2021-10-04},
	journal = {\apj},
	author = {Wylezalek, Dominika and Vernet, JoÃ«l and De Breuck, Carlos and Stern, Daniel and Brodwin, Mark and Galametz, Audrey and Gonzalez, Anthony H. and Jarvis, Matt and Hatch, Nina and Seymour, Nick and Stanford, Spencer A.},
	month = apr,
	year = {2014},
	note = {arXiv: 1403.2390},
	keywords = {Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics},
	pages = {17},
}

@article{papovich_angular_2008,
	title = {The {Angular} {Clustering} of {Distant} {Galaxy} {Clusters}},
	volume = {676},
	issn = {0004-637X, 1538-4357},
	url = {http://arxiv.org/abs/0712.1819},
	doi = {10.1086/527665},
	abstract = {We discuss the angular clustering of galaxy clusters at z {\textgreater} 1 selected within 50 deg2 from the Spitzer WideInfrared Extragalactic survey. We employ a simple color selection to identify high redshift galaxies with no dependence on galaxy restâframe optical color using Spitzer IRAC 3.6 and 4.5 Âµm photometry. The majority ({\textgreater}90\%) of galaxies with z {\textgreater} 1.3 are identiï¬ed with ([3.6] â [4.5])AB {\textgreater} â0.1 mag. We identify candidate galaxy clusters at z {\textgreater} 1 by selecting overdensities of â¥26â28 objects with [3.6] â [4.5] {\textgreater} â0.1 mag within radii of 1.4 arcminutes, which corresponds to r {\textless} 0.5 hâ1 Mpc at z = 1.5. These candidate galaxy clusters show strong angular clustering, with an angular correlation function represented by w(Î¸) = (3.1 Â± 0.5)(Î¸/1â²)â1.1Â±0.1 over scales of 2â100 arcminutes. Assuming the redshift distribution of these galaxy clusters follows a ï¬ducial model, these galaxy clusters have a spatialâclustering scale length r0 = 22.4 Â± 3.6 hâ1 Mpc, and a comoving number density n = 1.2 Â± 0.1 Ã 10â6h3 Mpcâ3. The correlation scale length and number density of these objects are comparable to those of rich galaxy clusters at low redshift. The number density of these highâredshift clusters correspond to darkâmatter halos larger than 3 â 5 Ã 1013 hâ1 Mâ at z = 1.5. Assuming the dark halos hosting these highâredshift clusters grow following ÎCDM models, these clusters will reside in halos larger than 1 â 2 Ã 1014 hâ1 Mâ at z = 0.2, comparable to rich galaxy clusters.},
	language = {en},
	number = {1},
	urldate = {2021-10-04},
	journal = {\apj},
	author = {Papovich, Casey},
	month = mar,
	year = {2008},
	note = {arXiv: 0712.1819},
	keywords = {Astrophysics},
	pages = {206--217},
}

@article{nayyeri_spitzer_2018,
	title = {Spitzer {Observations} of the {North} {Ecliptic} {Pole}},
	volume = {234},
	issn = {1538-4365},
	url = {http://arxiv.org/abs/1712.01290},
	doi = {10.3847/1538-4365/aaa07e},
	abstract = {We present a photometric catalog for Spitzer Space Telescope warm mission observations of the North Ecliptic Pole (NEP; centered at R.A. = 18h00m00s, Decl. = 66d33m38s.552). The observations are conducted with IRAC in 3.6 Âµm and 4.5 Âµm bands over an area of 7.04 deg2 reaching 1Ï depths of 1.29 ÂµJy and 0.79 ÂµJy in the 3.6 Âµm and 4.5 Âµm bands respectively. The photometric catalog contains 380,858 sources with 3.6 Âµm and 4.5 Âµm band photometry over the full-depth NEP mosaic. Point source completeness simulations show that the catalog is 80\% complete down to 19.7 AB. The accompanying catalog can be utilized in constraining the physical properties of extra-galactic objects, studying the AGN population, measuring the infrared colors of stellar objects, and studying the extra-galactic infrared background light.},
	language = {en},
	number = {2},
