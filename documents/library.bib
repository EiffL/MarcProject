	author = {McCracken, H. J. and Milvang-Jensen, B. and Dunlop, J. and Franx, M. and Fynbo, J. P. U. and FÃ¨vre, O. Le and Holt, J. and Caputi, K. I. and Goranova, Y. and Buitrago, F. and Emerson, J. P. and Freudling, W. and Hudelot, P. and LÃ³pez-Sanjuan, C. and Magnard, F. and Mellier, Y. and MÃ¸ller, P. and Nilsson, K. K. and Sutherland, W. and Tasca, L. and Zabl, J.},
