@article{cowley_galaxyhalo_2018,
	title = {The {Galaxy}â{Halo} {Connection} for \textdollar1.5\textbackslashlesssim z\textbackslashlesssim 5\textdollar as {Revealed} by {theSpitzerMatching} {Survey} of the {UltraVISTA} {Ultra}-deep {Stripes}},
