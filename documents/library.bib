@article{meneux_zcosmos_2009,
	title = {The {zCOSMOS} survey. {The} dependence of clustering on luminosity and stellar mass at z=0.2-1},
	volume = {505},
	issn = {0004-6361},
	url = {https://ui.adsabs.harvard.edu/abs/2009A%26A...505..463M/abstract},
	doi = {10.1051/0004-6361/200912314},
	abstract = {Aims: We study the dependence of galaxy clustering on luminosity and stellar mass at redshifts zË [0.2-1], using the first 10K redshifts from the zCOSMOS spectroscopic survey of the COSMOS field. {\textless}BR /{\textgreater}Methods: We measured the redshift-space correlation functions Î¾(r\_p,Ï) and Î¾(s) and the projected function, w\_p(r\_p) for subsamples covering different luminosity, mass, and redshift ranges. We explored and quantified in detail the observational selection biases from the flux-limited nature of the survey, using ensembles of realistic semi-analytic mock samples built from the Millennium simulation. We used the same mock data sets to carefully check our covariance and error estimate techniques, comparing the performances of methods based on the scatter in the mocks and on bootstrapping schemes. We finally compared our measurements to the cosmological model predictions from the mock surveys. {\textless}BR /{\textgreater}Results: At odds with other measurements at similar redshift and in the local Universe, we find a weak dependence of galaxy clustering on luminosity in all three redshift bins explored. A mild dependence on stellar mass is instead observed, in particular on small scales, which becomes particularly evident in the central redshift bin (0.5\&lt;z\&lt;0.8), where w\_p(r\_p) shows strong excess power on scales \&gt;1 h{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} Mpc. This is reflected in the shape of the full Î¾(r\_p,Ï) that we interpret as produced by dominating structures almost perpendicular to the line of sight in the survey volume. Comparing to zË 0 measurements, we do not see any significant evolution with redshift of the amplitude of clustering for bright and/or massive galaxies. {\textless}BR /{\textgreater}Conclusions: This is consistent with previous results and the standard picture in which the bias evolves more rapidly for the most massive haloes, which in turn host the highest-stellar-mass galaxies. At the same time, however, the clustering measured in the zCOSMOS 10K data at 0.5\&lt;z\&lt;1 for galaxies with log(M/M\_â)â¥ 10 is only marginally consistent with the predictions from the mock surveys. On scales larger than {\textasciitilde}2 h{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} Mpc, the observed clustering amplitude is compatible only with {\textasciitilde}1\% of the mocks. Thus, if the power spectrum of matter is ÎCDM with standard normalisation and the bias has no âunnaturalâ scale-dependence, this result indicates that COSMOS has picked up a particularly rare, {\textasciitilde}2-3Ï positive fluctuation in a volume of {\textasciitilde}10{\textless}SUP{\textgreater}6{\textless}/SUP{\textgreater} h{\textless}SUP{\textgreater}-1{\textless}/SUP{\textgreater} Mpc{\textasciicircum}3. These findings underline the need for larger surveys of the zË 1 Universe to appropriately characterise the level of structure at this epoch. {\textless}P /{\textgreater}Based on observation at the European Southern Observatory (ESO) {\textless}P /{\textgreater}Very Large Telescope (VLT) under Large Program 175.A-0839. {\textless}P /{\textgreater}Also based on observations with the NASA/ESA Hubble Space Telescope, {\textless}P /{\textgreater}obtained at the Space Telescope Science Institute, operated by the Association of Universities for Research in Astronomy, Inc. (AURA Inc.), {\textless}P /{\textgreater}under NASA contract NAS 5-26555, with the Subaru Telescope, {\textless}P /{\textgreater}operated by the National Astronomical Observatory of Japan, {\textless}P /{\textgreater}with the telescopes of the National Optical Astronomy Observatory, {\textless}P /{\textgreater}operated by the AURA {\textless}P /{\textgreater}under cooperative agreement with the National Science Foundation, {\textless}P /{\textgreater}and with the Canada-France-Hawaii Telescope, {\textless}P /{\textgreater}operated by the National Research Council of Canada, {\textless}P /{\textgreater}the Centre Nationla de la Recherche Scientifique de France {\textless}P /{\textgreater}and the University of Hawaii.},
	language = {en},
	number = {2},
	urldate = {2021-07-14},
	journal = {\aap},
	author = {Meneux, B. and Guzzo, L. and de la Torre, S. and Porciani, C. and Zamorani, G. and Abbas, U. and Bolzonella, M. and Garilli, B. and Iovino, A. and Pozzetti, L. and Zucca, E. and Lilly, S. J. and Le FÃ¨vre, O. and Kneib, J.-P. and Carollo, C. M. and Contini, T. and Mainieri, V. and Renzini, A. and Scodeggio, M. and Bardelli, S. and Bongiorno, A. and Caputi, K. and Coppa, G. and Cucciati, O. and de Ravel, L. and Franzetti, P. and Kampczyk, P. and Knobel, C. and KovaÄ, K. and Lamareille, F. and Le Borgne, J.-F. and Le Brun, V. and Maier, C. and PellÃ², R. and Peng, Y. and Perez Montero, E. and Ricciardelli, E. and Silverman, J. D. and Tanaka, M. and Tasca, L. and Tresse, L. and Vergani, D. and Bottini, D. and Cappi, A. and Cimatti, A. and Cassata, P. and Fumana, M. and Koekemoer, A. M. and Leauthaud, A. and Maccagni, D. and Marinoni, C. and McCracken, H. J. and Memeo, P. and Oesch, P. and Scaramella, R.},
	month = oct,
