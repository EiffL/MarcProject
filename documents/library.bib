@article{ashby_seds_2013,
	title = {{SEDS}: {THE} {SPITZER} {EXTENDED} {DEEP} {SURVEY}. {SURVEY} {DESIGN}, {PHOTOMETRY}, {AND} {DEEP} {IRAC} {SOURCE} {COUNTS}},
	volume = {769},
