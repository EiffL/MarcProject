@article{bocquet_halo_2016,
	title = {Halo mass function: baryon impact, fitting formulae, and implications for cluster cosmology},
	volume = {456},
	issn = {0035-8711},
	shorttitle = {Halo mass function},
	url = {https://ui.adsabs.harvard.edu/abs/2016MNRAS.456.2361B},
	doi = {10.1093/mnras/stv2657},
	abstract = {We use a set of hydrodynamical and dark matter-only (DMonly) simulations to calibrate the halo mass function (HMF). We explore the impact of baryons, propose an improved parametrization for spherical overdensity masses, and identify differences between our DMonly HMF and previously published HMFs. We use the Magneticum simulations, which are well suited because of their accurate treatment of baryons, high resolution, and large cosmological volumes of up to (3818 Mpc)3. Baryonic effects globally decrease the masses of galaxy clusters, which, at a given mass, results in a decrease of their number density. This effect vanishes at high redshift z â¼ 2 and for high masses M200 m â³ 1014 Mâ. We perform cosmological analyses of three idealized approximations to the cluster surveys by the South Pole Telescope (SPT), Planck, and eROSITA. We pursue two main questions. (1) What is the impact of baryons? - for the SPT-like and the Planck-like samples, the impact of baryons on cosmological results is negligible. In the eROSITA-like case, however, neglecting the baryonic impact leads to an underestimate of Î©m by about 0.01, which is comparable to the expected uncertainty from eROSITA. (2) How does our DMonly HMF compare with previous work? - for the Planck-like sample, results obtained using our DMonly HMF are shifted by Î(Ï8) â Î(Ï8(Î©m/0.27)0.3) â 0.02 with respect to results obtained using the Tinker et al. fit. This suggests that using our HMF would shift results from Planck clusters towards better agreement with cosmic-microwave-background anisotropy measurements. Finally, we discuss biases that can be introduced through inadequate HMF parametrizations that introduce false cosmological sensitivity.},
	urldate = {2021-12-14},
	journal = {\mnras},
	author = {Bocquet, Sebastian and Saro, Alex and Dolag, Klaus and Mohr, Joseph J.},
	month = mar,
	year = {2016},
	keywords = {Astrophysics - Cosmology and Nongalactic Astrophysics, cosmological parameters, cosmology: theory, dark matter, galaxies: clusters: general, large-scale structure of Universe, methods: numerical},
	pages = {2361--2373},
}

